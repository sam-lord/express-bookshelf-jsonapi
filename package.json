{
  "name": "express-bookshelf-jsonapi",
  "version": "0.3.2",
  "description": "Create JSON API endpoints from bookshelf",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=testing ./node_modules/.bin/mocha -- $(find test -name '*.test.js')",
    "coverage": "NODE_ENV=testing  ./node_modules/.bin/istanbul cover --dir test/coverage ./node_modules/.bin/_mocha -- $(find test -name '*.test.js')",
    "preship": "yarn test",
    "ship": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then yarn publish && git push --follow-tags; fi"
  },
  "alias": "ebja",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sam-lord/express-bookshelf-jsonapi.git"
  },
  "author": "Hannah Wolfe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sam-lord/express-bookshelf-jsonapi/issues"
  },
  "homepage": "https://github.com/sam-lord/express-bookshelf-jsonapi#readme",
  "dependencies": {
    "bookshelf-jsonapi-params": "1.5.3",
    "debug": "2.2.0",
    "express": "4.14.0",
    "ghost-ignition": "2.3.0",
    "jsonapi-mapper": "https://github.com/sam-lord/jsonapi-mapper/tarball/21cdf86218e42c17ae0ad71cb47740e12622c93e",
    "jsonapi-query-parser": "1.3.1",
    "lodash": "4.16.4",
    "merge-descriptors": "1.0.1"
  },
  "devDependencies": {
    "bluebird": "3.4.6",
    "chai": "3.5.0",
    "create-error": "0.3.1",
    "istanbul": "0.4.5",
    "mocha": "3.1.2",
    "sinon": "1.17.6",
    "sinon-bluebird": "3.1.0",
    "sinon-chai": "2.8.0"
  }
}
